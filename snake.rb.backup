class Snake < Formula
  desc "A simple snake game written in pure java"
  homepage "http://silo.cs.indiana.edu:32903/snake/snakehome.html"
  url "https://github.com/hasahmed/homebrew-hbrew/releases/download/2.0.0/simplesnake.tar.gz"
  sha256 "9886f049310dbcd6516b3bda081a082b7c86880251972f32a6b190bd3384257b"
  snakejar = "simplesnake.jar"

  def install
    ohai "#{prefix}"
    bin.install "snake"
    mv "simplesnake.tar.gz", "#{prefix}/"
    system "tar", "-xf", "#{prefix}", "simplesnake.tar.gz"
  end
  test do
    #system "java", "-jar", "#{prefix}/snake.jar", "--version"
  end
end
